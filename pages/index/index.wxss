/**index.wxss**/
@import "/wxSearch/wxSearch.wxss";
.main{
  background-color:rgba(223, 221, 218, 1);
}
.blank{
  width: 100%;
  height: 200rpx;
}
.tab{
  height: 100vh;
  width: 100vh;
  background-color: red;
}

.mask{
  position: fixed;
  background-color:#a19e9e;
  top:0;
  bottom: 0;
  left:0;
  right: 0;
  opacity: 0.70;
  display: none;
  z-index: 10;
}

.content{
  text-align: center;
}
.content text{
  line-height: 30px;
}
.content .flip{
  padding: 10px;
}
.box{
  background: #fff;
  height: 0px;
  overflow: hidden;
  z-index: 100;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
.nav_wrap{
  border-top: 1px solid rgb(187, 184, 184);
  background:rgb(239, 240, 240);
  position: flex;
  z-index: 200;
  bottom: 0;
  left:0rpx;
  height: 160rpx;
  width: 750rpx;
  overflow-y: hidden;
  display: flex;
  flex-direction: column;
  }
